<% i=1 %>
<% @transactions.each do |tr| %>
    <% account = Account.where(:csp_code => tr.csp_code).first %>
    <% if account.blank? %>
    <% else %>
        <% if account.account_number == 'N.A' || account.account_number == '00000000000' %>
        <% else %>
            <%= i %>
            <%= tr.csp_code %>
            <% amount = (-1) * tr.amount %>
            <%= amount= amount % 100 == 0 ? amount : amount - (amount % 100) %>
            W
            <% i = i + 1 %>
        <% end %>
    <% end %>
<% end %>
